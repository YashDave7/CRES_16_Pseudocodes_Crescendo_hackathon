    <h2 id="d">Health Personnel Registration</h2>
    <!-- <h4>"It's not how much we give,but how much love we put into giving" -Mother Teresa</h4> -->
    <div class="card-bunch">
      <!-- CARD TO ADD GOODS -->
      <div class="card" id="addgoods">
        <div class="container">
          <h3>Health Personnel</h3>
          <form action="/doctorSignup" method="post">
            <div class="form-control">
              <label for="text">Category</label><br />
              <!-- <input type="" id="text" placeholder="Enter Product Name..." /> -->
              <select id="mySelect" name="select" onchange="storeValue()" required>
                <option>Select</option>
                <option value="Doctor" id="indianbread">
                  Doctor
                </option>
                <option value="Pharmacist" id="rice">Pharmacist</option>
                <option value="Lab technician" id="pulses">Lab Technician</option>
                <option value="Medical records clerk" id="desserts">Medical records clerk</option>
            
                <!-- <option value="Cold Snacks" id="coldsnacks">Cold Snacks</option> -->
              </select>
            </div>
            <div class="form-control">
              <label for="quantity">UPR Number<br /> </label>
              <input
                type="text"
                id="quantity"
                min="1"
                placeholder="Enter UPR number"
                name="upr"
                required
                onchange="getQuantity()"
              />
            </div>
            <div class="form-control">
              <label for="quantity">Name<br /> </label>
              <input
                type="text"
                id="quantity"
                min="1"
                placeholder="Enter your full name"
                name="name"
                required
                onchange="getQuantity()"
              />
            </div>
            <div class="form-control">
              <label for="quantity">Username<br /> </label>
              <input
                type="text"
                id="quantity"
                min="1"
                placeholder="Enter username"
                name="username"
                required
                onchange="getQuantity()"
              />
            </div>
            <div class="form-control">
              <label for="quantity">Email<br /> </label>
              <input
                type="text"
                id="quantity"
                min="1"
                placeholder="Enter email"
                name="email"
                required
                onchange="getQuantity()"
              />
            </div>
            <div class="form-control">
              <label for="quantity">Password<br /> </label>
              <input
                type="text"
                id="quantity"
                min="1"
                placeholder="Enter password"
                name="password"
                required
                onchange="getQuantity()"
              />
            </div>
            <div class="form-control">
              <label for="quantity">Hospital/Clinic<br /> </label>
              <input
                type="text"
                id="quantity"
                min="1"
                placeholder="Enter Hospital/clinic name"
                name="hospital"
                required
                onchange="getQuantity()"
              />
            </div>
